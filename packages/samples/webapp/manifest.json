{
  "_version": "1.12.0",

  "sap.app": {
    "id": "dev.carlosorozco.ui5Extra.samples",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "UI5 TypeScript Hello World",
    "description": "UI5 TypeScript Hello World",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "set-ach"
  },

  "sap.fiori": {},

  "sap.ui": {
    "technology": "UI5",
    "icons": {},
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "rootView": {
      "viewName": "dev.carlosorozco.ui5Extra.samples.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },

    "dependencies": {
      "minUI5Version": "1.104.0",
      "libs": {
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.ui.unified": {},
        "sap.m": {}
      }
    },

    "handleValidation": true,

    "contentDensities": {
      "compact": true,
      "cozy": true
    },

    "models": {},

    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "viewPath": "dev.carlosorozco.ui5Extra.samples.view",
        "controlId": "layout",
        "controlAggregation": "beginColumnPages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "master",
          "target": "master"
        },
        {
          "pattern": "control/Text",
          "name": "Text",
          "target": ["master", "text"]
        },
        {
          "pattern": "control/Toast",
          "name": "Toast",
          "target": ["master", "toast"]
        },
        {
          "pattern": "control/ODataFetch",
          "name": "ODataFetch",
          "target": ["master", "odatafetch"]
        }
      ],
      "targets": {
        "master": {
          "viewName": "Master",
          "viewLevel": 1,
          "viewId": "master"
        },
        "text": {
          "viewName": "Text",
          "viewId": "text",
          "viewLevel": 1,
          "controlAggregation": "midColumnPages"
        },
        "toast": {
          "viewName": "Toast",
          "viewId": "toast",
          "viewLevel": 1,
          "controlAggregation": "midColumnPages"
        },
        "odatafetch": {
          "viewName": "ODataFetch",
          "viewId": "odatafetch",
          "viewLevel": 1,
          "controlAggregation": "midColumnPages"
        }
      }
    }
  }
}
