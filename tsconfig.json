{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "rootDir": "./",
    "outDir": "./src-gen",
    "baseUrl": "./",
    "paths": {
      "dev/carlosorozco/ui5Extra/*": ["./src/dev/carlosorozco/ui5Extra/*"]
    }
  },
  "include": ["./src/**/*", "./test/**/*"]
}
